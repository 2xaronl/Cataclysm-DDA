msgid_plural "Electromagnetic Unit CBMs"
msgstr[0] "Elektromagnes CBM"
msgstr[1] "Elektromagnes CBM"
msgstr[2] "Elektromagnes CBM"
msgstr[3] "Elektromagnes CBM"

#: lang/json/BIONIC_ITEM_from_json.py lang/json/bionic_from_json.py
