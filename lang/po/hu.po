msgstr "\"Van itt valaki?\""

#: lang/json/speech_from_json.py
msgid "\"Can you help me?\""
msgstr "\"Tudsz segíteni?\""

#: lang/json/speech_from_json.py
msgid "\"Over here!\""
msgstr "\"Itt vagyok!\""

#: lang/json/speech_from_json.py
msgid "\"Can you repeat that?\""
msgstr "\"Megismételnéd?\""

#: lang/json/speech_from_json.py
msgid "\"You're just copying me, aren't you?\""
msgstr "\"Csak utánozol engem, ugye?\""

#: lang/json/speech_from_json.py
msgid "\"I'm not afraid of you!\""
msgstr "\"Nem félek tőled!\""

#: lang/json/speech_from_json.py
msgid "\"Come here!\""
msgstr "\"Gyere ide!\""

#: lang/json/speech_from_json.py
