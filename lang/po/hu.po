msgstr "Gödör hegyes karókkal bélelése"

#: lang/json/construction_from_json.py
msgid "Glass Pit"
msgstr "Gödör szilánkokkal bélelése"

#: lang/json/construction_from_json.py
msgid "Fill Pit"
msgstr "Gödör betemetése"

#: lang/json/construction_from_json.py
msgid "Till Soil"
msgstr "Talaj felszántása"

